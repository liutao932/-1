//找出数组中出现次数大于n/2的数字
int majorityElement(int* nums, int numsSize){
    int cand = 0;
    int HP = 0;
    for(int i = 0; i<numsSize;i++)
    {
        if(HP == 0)
        {
            cand = nums[i];
            HP  = 1;
        }
        else if(nums[i] == cand)
        {
            HP++;
        }
        else
        {
            HP--;
        }
    }
    return cand;
}

